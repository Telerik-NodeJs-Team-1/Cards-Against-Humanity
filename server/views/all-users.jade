extend includes/layout
block main-content
    each player in users
        .row.text-center.centered-div.well(style="color:#000!important;margin-left:auto!important;margin-right:auto!important;font-size:16px")
            .col-md-6 #{player.firstName + ' ' + player.lastName} (#{player.username})
            .col-md-6
                a(href="/" class="btn btn-sm btn-primary") Edit
                | |
                a(href="/" class="btn btn-sm btn-primary") Delete